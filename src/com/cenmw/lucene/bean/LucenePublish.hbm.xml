<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tiao1.lucene.bean.LucenePublish" table="lucene_publish">
		<id name="id" type="integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="act" type="integer">
			<column name="act"></column>
		</property>

		<property name="status" type="integer">
			<column name="status"></column>
		</property>

		<property name="datatype" type="integer">
			<column name="datatype"></column>
		</property>
		<property name="infoid" type="integer">
			<column name="infoid"></column>
		</property>

	</class>
</hibernate-mapping>
